{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "defaultProject": "ld-demo",
  "projects": {
    "ld-demo": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ld-demo",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": { "browserTarget": "ld-demo:build" }
        }
      }
    },
    "launchdarkly-angular": {
      "projectType": "library",
      "root": "launchdarkly-angular",
      "sourceRoot": "launchdarkly-angular/src",
      "prefix": "ld",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "launchdarkly-angular/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "launchdarkly-angular/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "launchdarkly-angular/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "launchdarkly-angular/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "8aa03e36-44e4-409c-9898-455a61ed4e9a"
  }
}
